{
  "name": "RS ZB #10",
  "version": "2.6.0-1",
  "slug": "racksync-multipoint-zigbee-10",
  "description": "Run Zigbee2MQTT Coordinator separately",
  "uart": true,
  "url": "https://github.com/racksync/hass-addons-suite/tree/main/multipoint-zigbee/zb10",
  "startup": "application",
  "services": [
    "mqtt:need"
  ],
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "init": false,
  "ingress": true,
  "timeout": 30,
  "panel_icon": "mdi:numeric-10-circle",
  "map": [
    {
      "type": "share",
      "read_only": false
    },
    {
      "type": "homeassistant_config",
      "read_only": false,
      "path": "/config"
    },
    {
      "type": "addon_config",
      "read_only": false,
      "path": "/addon_config"
    }
  ],
  "ports": {
    "8485/tcp": 8495,
    "8099/tcp": null
  },
  "ports_description": {
    "8485/tcp": "Socat tcp-listen port (Don't change this port unless you know what you are doing!)",
    "8099/tcp": "Frontend tcp-listen port"
  },
  "options": {
    "data_path": "/config/racksync_zb_10",
    "socat": {
      "enabled": false,
      "master": "pty,raw,echo=0,link=/tmp/ttyZ2M,mode=777",
      "slave": "tcp-listen:8485,keepalive,nodelay,reuseaddr,keepidle=1,keepintvl=1,keepcnt=5",
      "options": "-d -d",
      "log": false
    },
    "mqtt": {},
    "serial": {
      "port": "tcp://192.168.10.20:6638",
      "adapter": "zstack",
      "baudrate": 115200,
      "disable_led": false,
      "advanced": {
        "transmit_power": 20
      }
    }
  },
  "schema": {
    "zigbee_herdsman_debug": "bool?",
    "data_path": "str",
    "socat": {
      "enabled": "bool?",
      "master": "str?",
      "slave": "str?",
      "options": "str?",
      "log": "bool?"
    },
    "mqtt": {
      "server": "str?",
      "ca": "str?",
      "key": "str?",
      "cert": "str?",
      "user": "str?",
      "password": "str?"
    },
    "serial": {
      "port": "str?",
      "adapter": "match(zstack|deconz|zigate|ezsp|ember)?",
      "baudrate": "int?",
      "rtscts": "bool?",
      "disable_led": "bool?",
      "advanced": {
        "transmit_power": "int?"
      }
    },
    "watchdog": "str?",
    "disable_tuya_default_response": "bool?"
  },
  "image": "ghcr.io/zigbee2mqtt/zigbee2mqtt-{arch}",
  "breaking_versions": [
    "2.0.0-1"
  ],
  "udev": true,
  "hassio_api": true
}